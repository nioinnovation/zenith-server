{
    "rules": {
        "no-console": [ 0 ],
        "comma-dangle": [ 2, "always-multiline" ],
        "indent": [ 0, 2 ],
        "quotes": [ 2, "single" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
        "curly": [ 2, "all" ],
        "eqeqeq": [ 2 ],
        "dot-notation": [ 2 ],
        "no-case-declarations": [ 2 ],
        "strict": [ 2, "global" ],
        "arrow-parens": [ 2, "always" ],
        "arrow-spacing": [ 2 ],
        "constructor-super": [ 2 ],
        "no-arrow-condition": [ 2 ],
        "no-class-assign": [ 2 ],
        "no-const-assign": [ 2 ],
        "no-dupe-class-members": [ 2 ],
        "no-this-before-super": [ 2 ],
        "no-eval": [ 2 ],
        "no-extend-native": [ 2 ],
        "no-floating-decimal": [ 2 ],
        "no-implicit-coercion": [ 2 ],
        "no-implied-eval": [ 2 ],
        "no-invalid-this": [ 2 ],
        "no-labels": [ 2 ],
        "no-multi-spaces": [ 2 ],
        "no-multi-str": [ 2 ],
        "no-native-reassign": [ 2 ],
        "no-new-func": [ 2 ],
        "no-new-wrappers": [ 2 ],
        "no-param-reassign": [ 2 ],
        "no-proto": [ 2 ],
        "no-return-assign": [ 2 ],
        "no-self-compare": [ 2 ],
        "no-throw-literal": [ 2 ],
        "no-void": [ 2 ],
        "no-with": [ 2 ],
        "wrap-iife": [ 2, "inside" ],
        "yoda": [ 2, "never" ],
        "no-shadow-restricted-names": [ 2 ],
        "no-shadow": [ 2 ],
        "no-use-before-define": [ 2, "nofunc" ],
        "no-mixed-requires": [ 2 ],
        "no-new-require": [ 2 ],
    },
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended"
}
