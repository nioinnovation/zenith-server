{
    "rules": {
        "no-console": [ 0 ],
        "comma-dangle": [ 2, "always-multiline" ],
        "indent": [ 2, 2 ],
        "quotes": [ 2, "single" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ]
    },
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended"
}
